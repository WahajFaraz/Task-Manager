{"version": 2, "builds": [{"src": "package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}], "routes": [{"src": "/api/(.*)", "dest": "https://task-manager-server.vercel.app/api/$1"}, {"src": "/assets/(.*)", "dest": "/assets/$1"}, {"src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|map))", "dest": "/$1"}, {"src": "/(.*)", "dest": "/index.html"}], "headers": [{"source": "/(.*\\.js)", "headers": [{"key": "Content-Type", "value": "application/javascript; charset=utf-8"}]}, {"source": "/(.*\\.css)", "headers": [{"key": "Content-Type", "value": "text/css; charset=utf-8"}]}, {"source": "/(.*\\.map)", "headers": [{"key": "Content-Type", "value": "application/json"}]}]}
